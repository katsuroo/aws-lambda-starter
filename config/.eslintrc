{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true
  },
  "extends": "airbnb-base",
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "no-console": "off",
    "no-multi-spaces": "off",
    "brace-style": ["error", "stroustrup", { "allowSingleLine": true }],
    "func-style": "off",
    "no-trailing-spaces": ["error", {"skipBlankLines": true}],
    "eol-last": "off",
    "key-spacing": "off"
  }
}
